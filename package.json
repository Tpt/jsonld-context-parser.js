{
  "name": "jsonld-context-flatten",
  "version": "1.0.0",
  "description": "Flattens JSON-LD contexts",
  "keywords": [
    "json",
    "linked data",
    "semantic web",
    "json-ld",
    "jsonld",
    "rdf"
  ],
  "main": "index.js",
  "typings": "index",
  "repository": "git@github.com:rubensworks/jsonld-context-flatten.js.git",
  "author": "Ruben Taelman <rubensworks@gmail.com>",
  "bugs": {
    "url": "https://github.com/rubensworks/jsonld-context-flatten.js/issues"
  },
  "homepage": "https://github.com/rubensworks/jsonld-context-flatten.js#readme",
  "license": "MIT",
  "files": [
    "lib/**/*.d.ts",
    "lib/**/*.js",
    "index.d.ts",
    "index.js"
  ],
  "pre-commit": [
    "build",
    "lint",
    "test"
  ],
  "devDependencies": {
    "@types/isomorphic-fetch": "^0.0.34",
    "@types/jest": "^23.3.0",
    "@types/node": "^10.5.2",
    "coveralls": "^3.0.0",
    "jest": "^23.4.1",
    "pre-commit": "^1.2.2",
    "ts-jest": "^23.0.1",
    "tslint": "^5.8.0",
    "tslint-eslint-rules": "^5.3.1",
    "typescript": "^2.6.2"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "jest-rdf",
    "transform": {
      "^.+\\.ts$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "collectCoverage": true
  },
  "scripts": {
    "test": "jest ${1}",
    "test-watch": "jest ${1} --watch",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "lint": "tslint index.ts lib/**/*.ts test/**/*.ts --exclude '**/*.d.ts'",
    "build": "tsc",
    "validate": "npm ls",
    "prepare": "npm run build"
  },
  "dependencies": {
    "isomorphic-fetch": "^2.2.1"
  }
}
